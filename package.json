{
  "name": "express-ws",
  "version": "1.0.0",
  "description": "tdd with Node",
  "main": "src/app.js",
  "scripts": {
    "start": "nodemon index --verbose",
    "test": "jest --watch",
    "lint": "eslint ."
  },
  "author": "Gerald Gillespie",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.1",
    "supertest": "^6.1.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "prettier/prettier": "warn",
      "eqeqeq": "warn"
    }
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  }
}
